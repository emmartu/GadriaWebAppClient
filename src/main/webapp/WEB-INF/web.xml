  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/resetAtlanteReport</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/ruleInputFileForm</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/rdfDataInputFileForm</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/testPage</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/itViewsPage</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
