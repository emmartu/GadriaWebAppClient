  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
  
  <session-config> 
    <session-timeout>-1</session-timeout>
  </session-config>
</web-app>