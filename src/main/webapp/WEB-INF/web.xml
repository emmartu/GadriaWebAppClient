<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>GadriaWebappClient</display-name>
  <welcome-file-list>
    <welcome-file>demoSettingsPage.html</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>webAppRootKey</param-name>
    <param-value>smart.monitoring.console.root</param-value>
  </context-param>
  <filter>
    <filter-name>characterEncodingFilter</filter-name>
    <filter-class>org.springframework.web.filter.CharacterEncodingFilter</filter-class>
    <init-param>
      <param-name>encoding</param-name>
      <param-value>UTF-8</param-value>
    </init-param>
    <init-param>
      <param-name>forceEncoding</param-name>
      <param-value>true</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>characterEncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <init-param>
      <param-name>contextConfigLocation</param-name>
      <param-value>/WEB-INF/spring/applicationContext.xml</param-value>
    </init-param>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/showsevents</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/eventsearch</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/processessearch</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/showrelatedprocess</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/showprocesses</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/showsaggregation</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/showChartsPanel</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/showsChart</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/showsconsole</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/atlanteconsole</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/loadAtlanteData</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/pauseAtlanteDataLoad</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/restartAtlanteDataLoad</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/loadReport</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SpringAppDispatcher</servlet-name>
    <url-pattern>/loadAtlanteDataForm</url-pattern>
  </servlet-mapping>

<!-- new bootstrap pages -->
  <servlet-mapping>
